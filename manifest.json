{
	"manifest_version": 2,
	"name": "dn-fix",
	"version": "0.1",

	"permissions": [
		"http://www.dn.se/*",
		"https://www.dn.se/*"
	],
	"content_scripts": [
		{
			"matches": ["http://www.dn.se/*", "https://www.dn.se/*"],
			"js": ["jquery-3.2.1.min.js", "fix.js"],
			"run_at": "document_end"
		}
	]
}